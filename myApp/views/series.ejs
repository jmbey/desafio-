<%- include('partials/head') %>

    <body>
    <%- include('partials/header') %>

    <div class="row row-cols-1 row-cols-md-3">
      <div class="col mb-4">
       <% for (let i=0; i < series.length; i++) { %>
         <div class="card">
             
         <div class="card-body">
           <a href =/series/detail/<%= series[i].id %> ><h5 class="card-title"> <%= series[i].title %> </h5> </a>
           <p class="card-text"> <%= series[i].rating %> </p>
         </div>
       </div>

     <% } %>
     </div>


     <!-- Esto deberia ser visible solo para users admin -->

     <div>
      <h3>No encontraste la serie?</h3>
      <a href=/series/create> Create </a>
  </div>
     

 </body>